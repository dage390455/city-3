<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="wrap_content">
<ImageView
    android:id="@+id/include_text_title_imv_arrows_left"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/arrows_left"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toBottomOf="parent"
    android:layout_marginStart="20dp"
    android:layout_marginTop="20dp"
    app:layout_goneMarginTop="10dp"
    android:layout_marginBottom="20dp"
    android:contentDescription="@null"
    />
    <TextView
        android:id="@+id/include_text_title_tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="预警日志"
        android:textSize="18sp"
        android:textColor="@color/c_030303"
        app:layout_constraintStart_toStartOf="@id/include_text_title_imv_arrows_left"
        app:layout_constraintTop_toTopOf="@id/include_text_title_imv_arrows_left"
        app:layout_constraintBottom_toBottomOf="@id/include_text_title_imv_arrows_left"
        android:layout_marginStart="12dp"
        />
    <TextView
        android:id="@+id/include_text_title_tv_subtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="历史日志"
        android:textSize="12sp"
        android:textColor="@color/c_a6a6a6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/include_text_title_imv_arrows_left"
        app:layout_constraintBottom_toBottomOf="@id/include_text_title_imv_arrows_left"
        android:layout_marginEnd="20dp"/>


</android.support.constraint.ConstraintLayout>