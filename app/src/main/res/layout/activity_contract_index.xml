<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/white"
              android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/contract_index_title_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y200"
        android:background="@color/white">

        <ImageView
            android:id="@+id/contract_index_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x10"
            android:adjustViewBounds="true"
            android:cropToPadding="true"
            android:padding="15dp"
            android:src="@mipmap/ic_back"/>

        <TextView
            android:id="@+id/contract_index_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="合同管理"
            android:textColor="@color/c_626262"
            android:textSize="@dimen/city_large_size"/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:background="@color/c_e7e7e7"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/c_f3f5f6"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/contract_index_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y60"
            android:background="@color/white"
            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/y1"
                android:layout_marginTop="@dimen/y30"
                android:background="@color/c_e7e7e7"
                android:contentDescription="@null"/>

            <RelativeLayout
                android:id="@+id/contract_index_name_license_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y160"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/iv_index_license"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:layout_alignParentLeft="true"
                    android:src="@mipmap/ic_contract_index_license"/>

                <TextView
                    android:id="@+id/contract_index_license_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/iv_index_license"
                    android:text="营业执照识别"
                    android:textColor="@color/c_626262"
                    android:textSize="@dimen/city_tiny_large_size"/>


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:adjustViewBounds="true"
                    android:cropToPadding="true"
                    android:padding="15dp"
                    android:src="@mipmap/ic_forward"/>
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/y1"
                android:layout_marginLeft="12dp"
                android:background="@color/c_e7e7e7"
                android:contentDescription="@null"/>

            <RelativeLayout
                android:id="@+id/contract_index_person_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y160"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/iv_index_person"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:layout_alignParentLeft="true"
                    android:src="@mipmap/ic_contract_index_person"/>

                <TextView
                    android:id="@+id/contract_index_person_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@id/iv_index_person"
                    android:text="身份证识别"
                    android:textColor="@color/c_626262"
                    android:textSize="@dimen/city_tiny_large_size"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:adjustViewBounds="true"
                    android:cropToPadding="true"
                    android:padding="15dp"
                    android:src="@mipmap/ic_forward"/>

            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/y1"
                android:layout_marginLeft="12dp"
                android:background="@color/c_e7e7e7"
                android:contentDescription="@null"/>

            <RelativeLayout
                android:id="@+id/contract_index_manual_rl"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y160"
                android:gravity="center_vertical">

                <ImageView
                    android:id="@+id/iv_index_manually"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:layout_alignParentLeft="true"
                    android:src="@mipmap/ic_contract_index_manually"/>

                <TextView
                    android:id="@+id/contract_index_manual_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/iv_index_manually"
                    android:text="手动填写业主信息"
                    android:textColor="@color/c_626262"
                    android:textSize="@dimen/city_tiny_large_size"/>


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:adjustViewBounds="true"
                    android:cropToPadding="true"
                    android:padding="15dp"
                    android:src="@mipmap/ic_forward"/>
            </RelativeLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="@dimen/y1"

                android:background="@color/c_e7e7e7"
                android:contentDescription="@null"/>
        </LinearLayout>


    </LinearLayout>
</LinearLayout>
