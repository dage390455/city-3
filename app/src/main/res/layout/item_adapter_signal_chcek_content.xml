<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginTop="16dp">
    <TextView
        android:id="@+id/item_adapter_signal_check_tv_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:textColor="@color/c_252525"/>
    <TextView
        android:id="@+id/item_adapter_signal_check_tv_up_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:drawablePadding="8dp"
        android:drawableStart="@drawable/signal_check_up_data"
        android:layout_marginTop="8dp"
        android:textColor="@color/c_5d5d5d"/>
    <TextView
        android:id="@+id/item_adapter_signal_check_tv_load_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:drawablePadding="8dp"
        android:layout_marginTop="8dp"
        android:drawableStart="@drawable/signal_check_load_data"
        android:textColor="@color/c_5d5d5d"/>


</LinearLayout>