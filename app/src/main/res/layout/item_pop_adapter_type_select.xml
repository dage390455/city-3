<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_pop_select_ll_root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="8dp"
    >
    <ImageView
        android:id="@+id/item_pop_select_imv_type_icon"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@mipmap/type_all_test"
        android:contentDescription="@null"
        android:layout_marginTop="8dp"
        android:layout_marginStart="8dp"
        />
    <TextView
        android:id="@+id/item_pop_select_tv_type_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/all_types"
        android:textSize="12sp"
        android:textColor="@color/c_252525"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        />


</LinearLayout>